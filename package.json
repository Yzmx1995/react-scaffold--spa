{
  "name": "react-scaffold",
  "version": "1.0.0",
  "description": "基于webpack4搭建的react单页应用脚手架",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "test": "npm run clean && node build/build production",
    "build:analyzer": "npm run clean && node build/build analyzer",
    "dev": "node build/dev-server development",
    "dev:staging": "node build/dev-server staging",
    "dev:production": "node build/dev-server production",
    "watch": "webpack-dev-server --inline --config build/webpack.dev.conf.js",
    "watch:dev": "npm run watch -- --NODE_ENV=development",
    "watch:staging": "npm run dev -- --NODE_ENV=staging",
    "watch:production": "npm run dev -- --NODE_ENV=production",
    "build": "webpack --config build/webpack.staging.conf.js",
    "build:staging": "npm run clean && cross-env NODE_ENV=staging  webpack --config build/webpack.staging.conf.js",
    "build:production": "npm run clean && cross-env NODE_ENV=production webpack --config build/webpack.production.conf.js",
    "clean": "rm -rf dist"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime-corejs3": "^7.11.2",
    "@hot-loader/react-dom": "^17.0.0-rc.2+4.13.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.3.0",
    "js-conditional-compile-loader": "^1.0.14",
    "less": "^3.12.2",
    "less-loader": "^6.2.0",
    "mini-css-extract-plugin": "^0.10.0",
    "node-notifier": "^8.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss": "^7.0.32",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^1.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "react": "^16.13.1",
    "core-js": "^3.6.5",
    "react-dom": "^16.13.1"
  }
}
